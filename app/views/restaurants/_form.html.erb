<%= form_with model: restaurant do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name, placeholder: "Type restaurant name" %>
   <%= f.label :address %>
   <%= f.text_field :address, placeholder: "Type restaurant address" %>
   <%= f.label :rating %>
   <%= f.number_field :rating, max: 5, min: 0 %>
   <%= f.submit %>
<% end %>